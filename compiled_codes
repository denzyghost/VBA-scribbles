Set Work_Separation = ActiveWorkbook
Sheets(1).Name = "Work_sheet_table"

Set Work_Separation = ActiveWorkbook
ActiveWorkbook.Sheets.Add.Name = ("NEWTYPE")

ActiveWorkbook.SaveAs ("Workbook_temporary")
